generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./discord.db"
}

model Guild {
  id      BigInt   @id @default(autoincrement())
  guildId BigInt @unique
  name    String
  members Int

  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt

  @@map("guilds")
}